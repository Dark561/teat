Draggable.Name = "Draggable"
Draggable.Parent = Gradient
 
task.spawn(function()
	local script = ToggleScript

	script.Parent.MouseButton1Down:Connect(function()
		local Toggled = script.Parent.Toggled
		
		if Toggled.Value == true then
			Toggled.Value = false
		else
			Toggled.Value = true
		end
		
		if Toggled.Value == true then
			script.Parent.crossA.Visible = false
			script.Parent.tickA.Visible = true
			
			script.Parent.FunctionScript.Enabled = true
		else
			script.Parent.tickA.Visible = false
			script.Parent.crossA.Visible = true
			
			script.Parent.FunctionScript.Enabled = false
		end
	end)
end)

task.spawn(function()
	local script = Draggable

	script.Parent.Active = true
	script.Parent.Draggable = true
	script.Parent.Visible = true
end)
